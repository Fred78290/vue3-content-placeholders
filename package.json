{
  "name": "vue3-content-placeholders",
  "description": "Vue3 Components for rendering animated content placeholders like facebook",
  "version": "1.0.0",
  "author": "Orignal author: Michał Sajnóg <msajnog93@gmail.com> (http://github.com/michalsnik/vue-content-placeholders.git), ported by Fred78290 http://github.com/Fred78290",
  "keywords": [
    "vue",
    "vuejs",
    "plugin",
    "loader",
    "fake",
    "placeholder",
    "progressive",
    "skeleton-ui"
  ],
  "license": "MIT",
  "main": "dist/vue3-content-placeholders.common.js",
  "unpkg": "dist/vue3-content-placeholders.browser.js",
  "module": "index.js",
  "scripts": {
    "serve": "vite",
    "build": "rm -rf dist && npm run build:browser && npm run build:common",
    "build:browser": "cross-env NODE_ENV=development vite build --config config/vite.config.browser.js",
    "build:common": "cross-env NODE_ENV=development vite build --config config/vite.config.common.js",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "publish": "npm publish --registry https://npm.aldunelabs.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fred78290/vue3-content-placeholders.git"
  },
  "bugs": {
    "url": "https://github.com/Fred78290/vue3-content-placeholders/issues"
  },
  "homepage": "https://github.com/Fred78290/vue3-content-placeholders#readme",
  "devDependencies": {
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@rushstack/eslint-patch": "^1.5.1",
    "@types/jsdom": "^21.1.3",
    "@types/node": "^18.17.17",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.50.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^22.1.0",
    "npm-run-all2": "^6.1.1",
    "sass": "^1.69.0",
    "vite": "^4.4.11",
    "vite-plugin-env-compatible": "^1.1.1",
    "vite-plugin-html": "3.2.0",
    "vite-plugin-singlefile": "^0.13.5",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.15"
  }
}
